{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".vercel/**",
        "generated-wundergraph/**",
        "generated/**"
      ]
    },
    "web#build": {
      "dependsOn": [
        "^api#build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "WG_ALLOWED_ORIGIN"
  ]
}